<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black">

    <ImageView
        android:id="@+id/soet"
        android:layout_width="135dp"
        android:layout_height="125dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="40dp"
        app:srcCompat="@drawable/engineering" />
    <ImageView
        android:id="@+id/fisheries"
        android:layout_width="135dp"
        android:layout_height="125dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="200dp"
        app:srcCompat="@drawable/fiesheries" />
    <ImageView
        android:id="@+id/media"
        android:layout_width="135dp"
        android:layout_height="125dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="360dp"
        app:srcCompat="@drawable/media" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="135dp"
        android:layout_height="125dp"
        android:layout_marginStart="220dp"
        android:layout_marginTop="40dp"
        app:srcCompat="@drawable/vocational" />

    <ImageView
        android:id="@+id/appliedScience"
        android:layout_width="135dp"
        android:layout_height="125dp"
        android:layout_marginStart="220dp"
        android:layout_marginTop="200dp"
        app:srcCompat="@drawable/appliedscience" />


    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="135dp"
        android:layout_height="125dp"

        android:layout_marginStart="220dp"
        android:layout_marginTop="360dp"
        app:srcCompat="@drawable/pharmacy" />

</RelativeLayout>